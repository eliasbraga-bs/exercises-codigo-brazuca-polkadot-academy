programa
{
	
	funcao inicio()
	{
		real number1, number2, number3, media

		escreva("Digite o primeiro número: ")
		leia(number1)

		escreva("Digite o segundo número: ")
		leia(number2)

		escreva("Digite o terceiro número: ")
		leia(number3)

		media = (number1 + number2 + number3) / 3

		escreva("A média dos números é: ", media)		
	}
}
